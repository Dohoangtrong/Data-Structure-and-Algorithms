#include <bits/stdc++.h>
using namespace std;

int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while(t--){
		int n, s, m;
		cin >> n >> s >> m;
		if ( 6*n < 7 *m || n < m ) cout << -1 << endl;
		else{
			int k = s*m;
			if ( k % n != 0 ){
				cout << k/n+1 << endl;
			}else{
				cout << k/n << endl;
			}
		}
	}
}

