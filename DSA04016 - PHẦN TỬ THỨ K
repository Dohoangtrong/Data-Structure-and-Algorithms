#include <bits/stdc++.h>
using namespace std;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while(t--){
    	int m, n, k;
    	cin >> m >> n >> k;
    	int cnt = 0, a;
    	vector<int> v;
    	for ( int i = 0; i < m+n; i++){
    		cin >> a;
    		v.push_back(a);
		}
		sort(v.begin(),v.end());
		cout << v[k-1] << endl;
	}
    
}
