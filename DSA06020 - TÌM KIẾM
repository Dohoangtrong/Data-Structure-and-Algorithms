#include <bits/stdc++.h>
using namespace std;

int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while(t--){
		int n, m;
		cin >> n >> m;
		int k = -1;
		vector<int> v(n);
		for ( int i = 0; i < n; i++){
			cin >> v[i];
		}
		for ( int i = n-1; i >= 0; i--){
			if ( v[i] == m ){
				k = 1;
				break;
			}
		}
		cout << k << endl;
	}
}


