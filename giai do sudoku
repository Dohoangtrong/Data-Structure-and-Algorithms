void in(){
	// in ra sudoku hoan chinh
	cout << endl;
	cout << "In sudoku ->" << endl;
	for ( int i = 0; i < 9; i++){
		for ( int j = 0; j < 9; j++){
			cout << arr[i][j];
			if ( j % 3 == 2 ){
				cout << "|";
			}else{
				cout << " ";
			}
		}
		cout << endl;
		if ( i % 3 == 2 ){
			cout << "-----+-----+-----|" << endl;
		}
	}
}
//    ---------dieu kien de thoa man ----------
bool Kiemtra( int giatri, int arr[9][9], int dong, int cot, int x, int y){
	for ( int i = 0; i < 9; i++){
		if ( giatri == arr[dong][i] && i != cot ){
			return false;
		}
		if ( giatri = arr[i][cot] && i != dong ){
			return false;
		}
	}
	
	for ( int i = x; i < 3*x+3; i++){
		for ( int j = y; j < 3*y+3; j++){
			if ( giatri == arr[i][j] && i != dong && j != cot ){
				return false;
			}
		}
	}
}
// ------------ giai thuat--------
void Giai(int c, int d){
	int a = 1;
	for ( int i = c; i < 9; i++){
		for ( int j = d; j < 9; j++){
			if (arr[i][j] == 0){
				if ( Kiemtra(a,arr,i,j,i/3,j/3) == false ){
					a++;
					Giai(c,d);
				}else{
					arr[i][j] = a;
					cout << 2;
					Giai(c,d+1);
				}
			}
		}
	}
}


main(){
//	nhap();
	Giai(0,0);
	in();
}
