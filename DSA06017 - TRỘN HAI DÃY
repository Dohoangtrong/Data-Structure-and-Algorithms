#include <bits/stdc++.h>
using namespace std;

int main(){
	int t;
	cin >> t;
	int x, y;
	while(t--){
		cin >> x >> y;
		vector<int> v;
		int a;
		for ( int i = 0; i < x+y; i++){
			cin >> a;
			v.push_back(a);
		}
		sort(v.begin(),v.end());
		for ( int i = 0; i < x+y; i++){
			cout << v[i] << " ";
		}
		cout << endl;
	}
}




