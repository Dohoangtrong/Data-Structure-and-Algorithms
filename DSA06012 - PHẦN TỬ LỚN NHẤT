#include <bits/stdc++.h>
using namespace std;

int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while(t--){
		int n, a;
		cin >> n >> a;
		vector<int> v;
		int c;
		for ( int i = 0; i < n; i++){
			cin >> c;
			v.push_back(c);
		}
		sort(v.begin(),v.end());
		for ( int i = 1; i <= a; i++){
			cout << v[n-i] << " ";
		}
		cout << endl;
	}
}

