#include <bits/stdc++.h>
using namespace std;

int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while(t--){
		int n, a;
		cin >> n >> a;
		int arr[1000];
		int cnt = 0;
		for ( int i = 0; i < n; i++){
			cin >> arr[i];
			if ( arr[i] == a ){
				cnt++;
			}
		}
		if ( cnt != 0 ){
			cout << cnt << endl;
		}else{
			cout << -1 << endl;
		}
	}
}

