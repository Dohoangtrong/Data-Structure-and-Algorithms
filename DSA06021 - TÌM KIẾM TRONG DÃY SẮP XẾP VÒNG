#include <bits/stdc++.h>
using namespace std;

int main(){
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	int t;
	cin >> t;
	while(t--){
		int n, m;
		cin >> n >> m;
		int a[n];
		for (int i = 0; i < n; i++)
			cin >> a[i];
		for ( int i = 0; i < n; i++)
			if ( a[i] == m ){
			cout << i+1 << endl;
			break;
			}
	}
}
