#include <bits/stdc++.h>
using namespace std;

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    cin.ignore();
    while(t--){
    	string a;
    	cin >> a;
    	int l = a.length();
    	int b = l-1;
    	while(a[b] == '0'){
    		a[b] = '1';
    		b--;
		}
		if ( b > -1 )
			a[b] = '0';
		cout << a << endl;
	}
}

